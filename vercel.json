{
    "version": 2,
    "builds": [
        {
            "src": "app/api/vercel.ts",
            "use": "@vercel/node"
        },
        {
            "src": "app/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "app/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app/api/vercel.ts"
        },
        {
            "src": "/(.*)",
            "dest": "app/dist/$1"
        }
    ]
}